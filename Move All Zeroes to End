void pushZerosToEnd(vector<int>& arr) {
       
        
        int i=0;
        int  j=0;
        int n=arr.size();
      while(i<=j && j<n){
          if(arr[j]!=0){
              arr[i]=arr[j];
              i++;
              j++;
              
          }
          else if(arr[j]==0){
              j++;   
          }
          
          
      }
      
      
      for(int k=i;k<n;k++){
          arr[k]=0;
      }
    }
